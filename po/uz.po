msgstr "5-ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:28
msgid "Switch to workspace 6"
msgstr "6-ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:32
msgid "Switch to workspace 7"
msgstr "7-ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:36
msgid "Switch to workspace 8"
msgstr "8-ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:40
msgid "Switch to workspace 9"
msgstr "9-ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:44
msgid "Switch to workspace 10"
msgstr "10-ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:48
msgid "Switch to workspace 11"
msgstr "11-ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:52
msgid "Switch to workspace 12"
msgstr "12-ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:56
msgid "Switch to workspace left"
msgstr "Chap tarafdagi ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:60
msgid "Switch to workspace right"
msgstr "O'ng tarafdagi ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:64
msgid "Switch to workspace above"
msgstr "Yuqoridagi ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:68
msgid "Switch to workspace below"
msgstr "Pastdagi ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:72
msgid "Switch to last workspace"
msgstr "Oxirgi ish maydoniga o'tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:76
msgid "Switch windows of an application"
msgstr "Ilova oynalarini almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:80
msgid "Reverse switch windows of an application"
msgstr "Ilova oynalarini teskari tartibda almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:84
msgid "Switch applications"
msgstr "Ilovalarni almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:88
msgid "Reverse switch applications"
msgstr "Ilovalarni teskari tartibda almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:92
msgid "Switch windows"
msgstr "Oynalarni almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:96
msgid "Reverse switch windows"
msgstr "Oynalarni teskari tartibda almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:100
msgid "Switch system controls"
msgstr "Tizim boshqaruv elementlarini almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:104
msgid "Reverse switch system controls"
msgstr "Tizim boshqaruv elementlarini teskari tartibda almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:108
msgid "Switch windows of an app directly"
msgstr "Ilova oynalarini to'g'ridan-to'g'ri almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:112
msgid "Reverse switch windows of an app directly"
msgstr "Teskari yo'nalishda ilova oynalarini to'g'ridan-to'g'ri almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:116
msgid "Switch windows directly"
msgstr "Oynalarni to'g'ridan-to'g'ri almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:120
msgid "Reverse switch windows directly"
msgstr "Teskari tartibda oynalarni to'g'ridan-to'g'ri almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:124
msgid "Switch system controls directly"
msgstr "Tizim boshqaruvlarini bevosita almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:128
msgid "Reverse switch system controls directly"
msgstr "Tizim boshqaruvlarini bevosita teskari tartibda almashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:132
msgid "Hide all normal windows"
msgstr "Barcha normal oynalarni yashirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:136
msgid "DEPRECATED: This key is deprecated and ignored."
msgstr "QO'LLANILMAYDI: Bu kalit eskirgan va inkor qilingan."

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:140
msgid "Show the run command prompt"
msgstr "Buyruq satrini chiqarish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:144
msgid "Don’t use"
msgstr "Foydalanmaslik"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:148
msgid "Activate the window menu"
msgstr "Oyna menyusini faollashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:152
msgid "Toggle fullscreen mode"
msgstr "Oynani to‘liq ekran rejimiga o‘tkazish yoki chiqarish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:156
msgid "Toggle maximization state"
msgstr "Oynaning maksimal holatini yoqish yoki o‘chirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:160
msgid "Toggle window always appearing on top"
msgstr "Oynani doimo yuqorida ko‘rinishini yoqish yoki o‘chirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:164
msgid "Maximize window"
msgstr "Oynani kattalashtirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:168
msgid "Restore window"
msgstr "Oynani tiklash"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:172
msgid "Minimize window"
msgstr "Oynani kichraytirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:176
msgid "Close window"
msgstr "Oynani yopish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:180
msgid "Move window"
msgstr "Oynani surish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:184
msgid "Resize window"
msgstr "Oyna o'lchamini o'zgartirish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:188
msgid "Toggle window on all workspaces or one"
msgstr ""
"Oynani barcha ish maydonlarida ko‘rsatish yoki faqat bittasida ko‘rsatishga "
"o‘tish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:192
msgid "Move window to workspace 1"
msgstr "Oynani 1-ish maydoniga o'tkazish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:196
msgid "Move window to workspace 2"
msgstr "Oynani 2-ish maydoniga o'tkazish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:200
msgid "Move window to workspace 3"
msgstr "Oynani 3-ish maydoniga o'tkazish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:204
msgid "Move window to workspace 4"
msgstr "Oynani 4-ish maydoniga o'tkazish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:208
msgid "Move window to workspace 5"
msgstr "Oynani 5-ish maydoniga o'tkazish"

#: schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in:212
msgid "Move window to workspace 6"
